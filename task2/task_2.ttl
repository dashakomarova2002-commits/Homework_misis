@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.org/genealogy#> .

<http://example.org/genealogy> rdf:type owl:Ontology ;
                                rdfs:label "Генеалогическое дерево"@ru .

#################################################################
#    Object Properties
#################################################################

###  http://example.org/genealogy#hasAncestor
:hasAncestor rdf:type owl:ObjectProperty ;
             owl:inverseOf :hasDescendant ;
             rdf:type owl:TransitiveProperty ;
             rdfs:domain :Human ;
             rdfs:range :Human ;
             rdfs:label "имеет предка"@ru .


###  http://example.org/genealogy#hasDescendant
:hasDescendant rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ;
               rdfs:domain :Human ;
               rdfs:range :Human ;
               rdfs:label "имеет потомка"@ru .


###  http://example.org/genealogy#hasParent
:hasParent rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasAncestor ;
           rdf:type owl:FunctionalProperty ,
                    owl:AsymmetricProperty ,
                    owl:IrreflexiveProperty ;
           rdfs:domain :Human ;
           rdfs:range :Human ;
           rdfs:label "имеет родителя"@ru .


#################################################################
#    Classes
#################################################################

###  http://example.org/genealogy#Human
:Human rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :hasParent ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :Human
                       ] ;
       rdfs:label "Человек"@ru .


#################################################################
#    Individuals
#################################################################

###  http://example.org/genealogy#Владимир
:Владимир rdf:type owl:NamedIndividual .


###  http://example.org/genealogy#Дарья
:Дарья rdf:type owl:NamedIndividual .


###  http://example.org/genealogy#Екатерина
:Екатерина rdf:type owl:NamedIndividual ;
           :hasDescendant :Дарья ;
           :hasParent :Владимир .


